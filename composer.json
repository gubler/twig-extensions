{
    "name": "gubler/twig-extensions",
    "type": "library",
    "description": "Twig extensions for internal projects",
    "keywords": [
        "twig",
        "html"
    ],
    "homepage": "https://github.com/gubler/twig-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Daryl Gubler",
            "email": "daryl@dev88.co",
            "homepage": "http://dev88.co"
        }
    ],
    "support": {
        "issues": "https://github.com/gubler/twig-extensions/issues",
        "source": "https://github.com/gubler/twig-extensions"
    },
    "require": {
        "php": "^7.2.5",
        "twig/twig": "^2.4|^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Gubler\\Twig\\Extension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gubler\\Twig\\Extension\\Tests\\": "tests/"
        }
    }
}